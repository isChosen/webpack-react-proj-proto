{
  "name": "hotel_admin",
  "version": "3.0.0",
  "description": "refactor",
  "main": "index.js",
  "keywords": [
    "react",
    "redux",
    "webpack",
    "ant design",
    "react-router4"
  ],
  "author": "liangchaoshun",
  "license": "ISC",
  "browserslist": [
    "> 0.2%"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-proposal-async-generator-functions",
      "@babel/plugin-transform-runtime",
      [
        "import",
        {
          "style": "css",
          "libraryName": "antd",
          "libraryDirectory": "es"
        }
      ]
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": {},
      "autoprefixer": {}
    }
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-async-generator-functions": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.4.6",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "cssnano": "^4.1.8",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/runtime": "^7.3.1",
    "antd": "^3.13.0",
    "axios": "^0.18.0",
    "echarts": "^4.1.0",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.1",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^1.0.1"
  },
  "scripts": {
    "dll:dev": "webpack --progress --config webpack.config.dll.js --env.NODE_ENV=development",
    "dll:pro": "webpack --progress --config webpack.config.dll.js --env.NODE_ENV=production",
    "start:self": "webpack-dev-server --config webpack.config.main.dev.js --env.NODE_ENV=development",
    "build:self": "webpack --progress --config webpack.config.main.build.js --env.NODE_ENV=production",
    "start": "npm run dll:dev && npm run start:self",
    "build": "npm run dll:pro && npm run build:self"
  }
}
